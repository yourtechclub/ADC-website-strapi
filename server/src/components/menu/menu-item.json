{
  "collectionName": "components_menu_menu_items",
  "info": {
    "displayName": "Menu Item",
    "description": "Single navigation menu item with label, URL, and order"
  },
  "options": {},
  "attributes": {
    "label": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "url": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 10,
      "min": 1,
      "max": 99
    },
    "isExternal": {
      "type": "boolean",
      "default": false
    }
  }
}
